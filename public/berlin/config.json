{
  "version": 8,
  "sources": {
    "bezirke": {
      "type": "vector",
      "tiles": [
        "http://localhost:10060/v2/tiles/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "layers": [{
    "id": "background",
    "type": "background",
    "paint": {
      "background-color": "#ffa"
    }
  },{
    "id": "Bezirk",
    "type": "line",
    "source": "bezirke",
    "source-layer": "Bezirksregion",
    "layout": {
      "line-join":"round"
    },
    "paint": {
      "line-color": "red",
      "line-width": 5,
      "line-opacity": {
        "stops": [[0,0],[1,0.1]]
      }
    }
  }]
}
